<!DOCTYPE html>
<html lang="en">
<title>{{title}}</title>
<h1>{{title}}</h1>
<main>
    <h2>{{searchTerm}}</h2>
    <ul>
        {{#each showsFound}}
            <li>
                <a href="/shows/{{this.id}}">{{this.name}}</a>
            </li>
        {{/each}}
    </ul>
    <a href="/">Make another search</a>
</main>
</html>